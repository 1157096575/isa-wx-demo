@import "../../../../static/common.wxss";
.search-box-wrap{
  position: relative;
  height:80rpx;
}
.search-box{
  padding:0 40rpx;
  /* position: fixed; */
  left:0;
  top:140rpx;
  width: 100%;
  z-index: 88;
  background: #fff;
}
.search-input-box{
  border-radius: 10rpx;
  background: #f7f8fa;
  height:70rpx;
  width: 100%;
  position: relative;
  padding: 10rpx 60rpx;
  margin-top:10rpx;
}
.search-box .search-input{
  width: 100%;
  height:50rpx;
  line-height: 50rpx;
}
.search-icon{
  position: absolute;
  top:18rpx;
  left:20rpx;
}
.close-btn{
  position: absolute;
  top:22rpx;
  right:20rpx;
}
.placeholder-class{
  font-size: 26rpx;
}